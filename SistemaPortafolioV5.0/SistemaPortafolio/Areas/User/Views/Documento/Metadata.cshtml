                            <th>Palabra Total</th>
                            <th>Caracter Total</th>
                            <th>Línea Total</th>
                            <th>Párrafo Total</th>
                            <th>Tamaño</th>
                            <th>Fecha de Subida</th>
                        </tr>
                    </thead>
