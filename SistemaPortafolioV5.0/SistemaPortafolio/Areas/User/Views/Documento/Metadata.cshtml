                            {
                                <th class="alert-danger"><b class="text-black">-@cartotal.ToString()</b></th>
                            }
                            else
                            {
                                <th class="alert-success"><b class="text-black">+@cartotal.ToString()</b></th>
                            }

                            @if (Convert.ToInt32(lintotal) < 0)
