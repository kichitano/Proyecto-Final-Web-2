    string paltotal = "";
    string cartotal = "";
    string lintotal = "";
    string parrtotal = "";
    string taman = "";
    string celda = "";
    string columna = "";
    string fecha = "";
}
<div class="panel panel-default">
    <div class="panel-body">
        <div class="panel panel-default">
            <div class="panel-heading">Registro de Metadata del Documento</div>
        </div>

